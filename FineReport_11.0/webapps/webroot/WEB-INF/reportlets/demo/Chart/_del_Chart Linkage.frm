<TableDataMap>
<TableData name="total sales" class="com.fr.data.impl.DBTableData">
<Parameters/>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[FRDemo]]></DatabaseName>
</Connection>
<Query>
<![CDATA[select substr(SIGNDATE,1,4)as year, sum(PRICE*QUANTITY) as total_sales
from  ORDERS, ORDERSDETAIL, EMPLOYEE
where ORDERS.ORDERID = ORDERSDETAIL.ORDERID 
	and ORDERS.EMPID = EMPLOYEE.EMPID
group by year
order by year]]></Query>
<PageQuery>
<![CDATA[]]></PageQuery>
</TableData>
<TableData name="sales by employee @ year" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="year"/>
<O>
<![CDATA[2009]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[FRDemo]]></DatabaseName>
</Connection>
<Query>
<![CDATA[select EMPNAME , sum(price*quantity) as total_sales
from ORDERS, ORDERSDETAIL, EMPLOYEE
where ORDERS.ORDERID = ORDERSDETAIL.ORDERID and ORDERS.EMPID = EMPLOYEE.EMPID
	and substr(SIGNDATE,1,4) = '${year}'
group by EMPNAME
order by total_sales desc
]]></Query>
<PageQuery>
<![CDATA[]]></PageQuery>
</TableData>
<TableData name="orders by employee @ year" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="year"/>
<O>
<![CDATA[2008]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[FRDemo]]></DatabaseName>
</Connection>
<Query>
<![CDATA[select EMPNAME, COUNT(ORDERS.EMPID) as total_orders
from ORDERS,EMPLOYEE
where ORDERS.EMPID = EMPLOYEE.EMPID and substr(SIGNDATE,1,4) = '${year}'
group by EMPNAME
order by total_orders desc]]></Query>
<PageQuery>
<![CDATA[]]></PageQuery>
</TableData>
<TableData name="return rate @ year" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="year"/>
<O>
<![CDATA[2008]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[FRDemo]]></DatabaseName>
</Connection>
<Query>
<![CDATA[select substr(SIGNDATE,1,6) as month, round(sum(PAID)/sum(AMOUNT),2) as return_rate
from ORDERS
where substr(SIGNDATE,1,4) = '${year}'
group by month]]></Query>
<PageQuery>
<![CDATA[]]></PageQuery>
</TableData>
